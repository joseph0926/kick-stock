{
  "name": "@kickstock/ssr",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
    "dev:ssr": "concurrently -n \"server,client\" -c \"blue,green\" \"tsx watch --clear-screen=false ./src/index.ts\" \"cd ../web && vite build --watch\"",
    "lint": "eslint .",
    "format": "prettier --w \"**/*.{js,jsx,ts,tsx}\"",
    "build:ssr": "tsc -b && vite build",
    "clean": "rimraf node_modules"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/static": "^8.0.3",
    "@kickstock/core": "workspace:*",
    "@kickstock/shared": "workspace:*",
    "@sinclair/typebox": "^0.34.13",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3"
  }
